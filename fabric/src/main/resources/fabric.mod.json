{
  "schemaVersion": 1,
  "id": "athena",
  "version": "${version}",
  "name": "Athena",
  "description": "Crossplatform Baked Model Loader",
  "authors": [
    "ThatGravyBoat"
  ],
  "contributors": [
  ],
  "contact": {
    "website": "https://www.curseforge.com/minecraft/mc-mods/athena",
    "repo": "https://github.com/terrarium-earth/${github_name}",
    "issues": "https://github.com/terrarium-earth/${github_name}/issues"
  },
  "license": "Terrarium Licence",
  "icon": "icon.png",
  "environment": "*",
  "mixins": [
      "athena.mixins.json"
  ],
  "entrypoints": {
